    "import cv2\n",
    "import numpy as np\n",
    "  \n",
    "img = cv2.imread('exercise1.png')\n",
    "  \n",
    "for gamma in [3.0, 4.0, 5.0]:\n",
    "    gamma_corrected = np.array(255*(img / 255) ** gamma, dtype = 'uint8')\n",
    "    cv2.imwrite('gamma_result'+str(gamma)+'.jpg', gamma_corrected)"
   ]
  }
